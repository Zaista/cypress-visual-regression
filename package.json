{
  "name": "cypress-visual-regression",
  "version": "3.0.0",
  "description": "Module for adding visual regression testing to Cypress",
  "keywords": [
    "visual regression",
    "image diff",
    "cypress"
  ],
  "repository": "https://github.com/cypress-visual-regression/cypress-visual-regression",
  "license": "MIT",
  "author": "Michael Herman <michael@mherman.org>",
  "types": "dist/command.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.json",
    "ci": "export SNAPSHOT_DIRECTORY=cypress/snapshots && npm run build && npm run base && npm test",
    "jest": "jest tests",
    "lint": "eslint src",
    "prepare": "npm run build",
    "prettier:check": "prettier --check src/**/*.js",
    "prettier:fix": "prettier --write src/**/*.js",
    "prepublish": "npm run jest && npm run lint && npm run prettier:check && npm run build",
    "vr:base": "cypress run --env type=base",
    "vr:test": "cypress run --env type=actual"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "dependencies": {
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "sanitize-filename": "^1.6.3"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.2",
    "@tsconfig/node18": "1.0.1",
    "@tsconfig/strictest": "2.0.0",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.1",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "5.0.3"
  },
  "peerDependencies": {
    "cypress": ">=9.7.0"
  },
  "ignore": [
    "src",
    "docker",
    "tests"
  ]
}
