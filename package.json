{
  "name": "cypress-visual-regression",
  "version": "3.0.0",
  "description": "Module for adding visual regression testing to Cypress",
  "keywords": [
    "visual regression",
    "image diff",
    "cypress"
  ],
  "repository": "https://github.com/cypress-visual-regression/cypress-visual-regression",
  "license": "MIT",
  "author": "Michael Herman <michael@mherman.org>",
  "types": "dist/command.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.json",
    "ci": "export SNAPSHOT_DIRECTORY=cypress/snapshots && npm run build && npm run base && npm test",
    "format:check": "prettier --check src/**/*.ts",
    "format:fix": "prettier --write src/**/*.ts",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "run-s build",
    "preversion": "run-s lint format:check test build",
    "test": "vitest run",
    "vr:base": "cypress run --env type=base",
    "vr:test": "cypress run --env type=actual"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/tests/**/*.spec.js"
    ]
  },
  "dependencies": {
    "debug": "^4.3.4",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "sanitize-filename": "^1.6.3",
    "serialize-error": "11.0.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@tsconfig/strictest": "2.0.0",
    "@types/debug": "4.1.7",
    "@types/jest": "29.5.0",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.7",
    "rimraf": "^3.0.2",
    "ts-jest": "29.1.0",
    "typescript": "5.0.3",
    "vitest": "0.29.8"
  },
  "peerDependencies": {
    "cypress": ">=9.7.0"
  },
  "engines": {
    "node": ">=18"
  },
  "ignore": [
    "src",
    "docker",
    "tests"
  ],
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  }
}
